{
  "name": "create-creative-app",
  "version": "0.1.0",
  "description": "Interactive wizard to scaffold a creative Vite + React app",
  "type": "module",
  "engines": {
    "node": ">18"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@clack/prompts": "^0.11.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/fs-extra": "^11.0.4",
    "chalk": "^5.6.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "ignore": "^7.0.5",
    "mri": "^1.2.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@eslint/js": "^9.35.0",
    "@types/babel__generator": "^7.27.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "scripts": {
    "build": "ncc build src/index.ts -o dist -m",
    "test:publish": "npm run build && node scripts/test-publish.js",
    "create": "node dist/index.js",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist",
    "template"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.{js,ts})": "prettier --write --ignore-unknown"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-ming/npx.git"
  },
  "keywords": [],
  "author": "tim-ming",
  "bugs": {
